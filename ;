#include <iostream>
#include <vector>

#include "aux.hpp"
#include "layers.hpp"
#include "loss.hpp"

using std::vector;

int main(void) {
  
    vector<vector<double>> input(1, vector<double>{1,2,3,4,5});
    Dense<> d1(5, 5);
    vector<vector<double>> hidden_scores = d1.forward_pass(input);
    Dense<> d2(5, 5);
    vector<vector<double>> scores = d2.forward_pass(hidden_scores);

    vector<vector<double>> actual{{0,0,0,1,0}};
    std::cout << loss(scores, actual); 
}
